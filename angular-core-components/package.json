    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "istanbul": "^0.4.5",
    "istanbul-api": "^3.0.0",
    "istanbul-lib-coverage": "^3.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
