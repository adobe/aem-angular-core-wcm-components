{
  "name": "@adobe/cq-root-angular-editable-components",
  "version": "2.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build cq-angular-editable-components",
    "build:production": "ng build cq-angular-editable-components --prod",
    "test": "ng test cq-angular-editable-components --code-coverage",
    "test:debug": "ng test cq-angular-editable-components --watch=true --code-coverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "publish-lib": "ng build cq-angular-editable-components && cd dist/cq-angular-editable-components && npm publish",
    "version": "npm run docs && node update-doc.js && npm run readme && rm DOCUMENTATION.md && cp README.md dist/cq-angular-editable-components/ && git add -A",
    "preversion": "npm run build:production && npm run test",
    "postversion": "git push && git push --tags",
    "docs": "node_modules/.bin/typedoc --theme markdown --options typedoc.js projects/cq-angular-editable-components/src/lib --mode file --target ES6 --mdHideSources",
    "readme": "node_modules/markdown-include/bin/cli.js markdown-include.config.json"
  },
